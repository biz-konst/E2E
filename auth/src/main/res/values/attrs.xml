<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressButtonLayout">
        <attr name="pbInsetLeft" format="dimension" />
        <attr name="pbInsetTop" format="dimension" />
        <attr name="pbInsetRight" format="dimension" />
        <attr name="pbInsetBottom" format="dimension" />
        <attr name="pbGravity" format="integer">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="center" value="0" />
            <enum name="start" value="-1" />
            <enum name="end" value="-2" />
